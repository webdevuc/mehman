    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ngx-unused-css": "^4.0.0-1",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "typescript": "^4.6.4"
  },
  "browser": {
    "crypto": false
