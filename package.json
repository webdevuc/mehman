{
  "name": "mehman-booking-engine",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "compile": "ngc",
    "start": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --open ",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build && gzipper compress ./dist",
    "test": "ng test",
    "lint": "tslint src/**/*.ts --config tslint.json --project tsconfig.json ",
    "lint:fix": "tslint src/**/*.ts --config tslint.json --fix --project tsconfig.json",
    "e2e": "ng e2e",
    "unused-css": "ngx-unused-css",
    "analyze": "source-map-explorer 'dist/mehman-booking-engine/*.js'",
    "build:stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --stats-json",
    "build:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production  --output-hashing=all && gzipper compress ./dist",
    "bundle:report": "webpack-bundle-analyzer dist/mehman-booking-engine/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^14.1.1",
    "@angular/animations": "^14.1.1",
    "@angular/cdk": "^12.1.1",
